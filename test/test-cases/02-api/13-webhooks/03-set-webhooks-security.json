{
    "name": "set-webhooks-security",
    "instructions": [
		{
			"name": "json-http-client",
			"params": {
				"method": "PATCH",
				"path": "/webhooks",
				"headers": {
					"Authorization": "Bearer inexistent"
				},
				"body": {
				    "enabled": false
				},
				"expected_status": 401
			}
		},
        {
            "name": "json-http-client",
            "params": {
                "method": "PATCH",
                "path": "/webhooks",
                "headers": {
                    "Authorization": "Bearer ${TEST_JWT_VIEWONLY}"
                },
				"body": {
				    "enabled": false
				},
                "expected_status": 403
            }
        },
        {
            "name": "json-http-client",
            "params": {
                "method": "PATCH",
                "path": "/webhooks",
                "headers": {
                    "Authorization": "Bearer ${TEST_JWT_NORMAL}"
                },
				"body": {
				    "enabled": false
				},
                "expected_status": 403
            }
        }
    ]
}

