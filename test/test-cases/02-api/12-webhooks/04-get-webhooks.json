{
    "name": "get-webhooks",
    "instructions": [
        {
            "name": "json-http-client",
            "params": {
                "method": "GET",
                "path": "/webhooks",
                "headers": {
                    "Authorization": "Bearer ${TEST_JWT_ADMIN}"
                },
                "expected_status": 200,
                "expected_body": {
                    "enabled": true,
                    "scheme": "http",
                    "host": "{{HOST_IP_ADDRESS}}",
                    "port": 8080,
                    "path": "/webhooks",
                    "events": [
                        "value-change",
                        "port-update",
                        "port-add",
                        "port-remove",
                        "device-update",
                        "full-update"
                    ],
                    "timeout": 2,
                    "retries": 0
                }
            }
        }
    ]
}

