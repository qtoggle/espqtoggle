{
    "name": "expressions-bitwise",
    "instructions": [
		{
			"name": "json-http-client",
			"params": {
				"method": "PATCH",
				"path": "/ports/num_port",
				"headers": {
					"Authorization": "Bearer ${TEST_JWT_ADMIN}"
				},
				"body": {
				    "expression": "BITAND(11, 12)"
				},
				"expected_status": 204
			}
		},
		{
			"name": "json-http-client",
			"params": {
				"method": "GET",
				"path": "/ports/num_port/value",
				"headers": {
					"Authorization": "Bearer ${TEST_JWT_ADMIN}"
				},
				"expected_status": 200,
				"expected_body": 8
			}
		},
		{
			"name": "json-http-client",
			"params": {
				"method": "PATCH",
				"path": "/ports/num_port",
				"headers": {
					"Authorization": "Bearer ${TEST_JWT_ADMIN}"
				},
				"body": {
				    "expression": "BITOR(11, 12)"
				},
				"expected_status": 204
			}
		},
		{
			"name": "json-http-client",
			"params": {
				"method": "GET",
				"path": "/ports/num_port/value",
				"headers": {
					"Authorization": "Bearer ${TEST_JWT_ADMIN}"
				},
				"expected_status": 200,
				"expected_body": 15
			}
		},
		{
			"name": "json-http-client",
			"params": {
				"method": "PATCH",
				"path": "/ports/num_port",
				"headers": {
					"Authorization": "Bearer ${TEST_JWT_ADMIN}"
				},
				"body": {
				    "expression": "BITNOT(11)"
				},
				"expected_status": 204
			}
		},
		{
			"name": "json-http-client",
			"params": {
				"method": "GET",
				"path": "/ports/num_port/value",
				"headers": {
					"Authorization": "Bearer ${TEST_JWT_ADMIN}"
				},
				"expected_status": 200,
				"expected_body": -12
			}
		},
		{
			"name": "json-http-client",
			"params": {
				"method": "PATCH",
				"path": "/ports/num_port",
				"headers": {
					"Authorization": "Bearer ${TEST_JWT_ADMIN}"
				},
				"body": {
				    "expression": "BITXOR(11, 12)"
				},
				"expected_status": 204
			}
		},
		{
			"name": "json-http-client",
			"params": {
				"method": "GET",
				"path": "/ports/num_port/value",
				"headers": {
					"Authorization": "Bearer ${TEST_JWT_ADMIN}"
				},
				"expected_status": 200,
				"expected_body": 7
			}
		},
		{
			"name": "json-http-client",
			"params": {
				"method": "PATCH",
				"path": "/ports/num_port",
				"headers": {
					"Authorization": "Bearer ${TEST_JWT_ADMIN}"
				},
				"body": {
				    "expression": "SHL(111, 3)"
				},
				"expected_status": 204
			}
		},
		{
			"name": "json-http-client",
			"params": {
				"method": "GET",
				"path": "/ports/num_port/value",
				"headers": {
					"Authorization": "Bearer ${TEST_JWT_ADMIN}"
				},
				"expected_status": 200,
				"expected_body": 888
			}
		},
		{
			"name": "json-http-client",
			"params": {
				"method": "PATCH",
				"path": "/ports/num_port",
				"headers": {
					"Authorization": "Bearer ${TEST_JWT_ADMIN}"
				},
				"body": {
				    "expression": "SHR(111, 3)"
				},
				"expected_status": 204
			}
		},
		{
			"name": "json-http-client",
			"params": {
				"method": "GET",
				"path": "/ports/num_port/value",
				"headers": {
					"Authorization": "Bearer ${TEST_JWT_ADMIN}"
				},
				"expected_status": 200,
				"expected_body": 13
			}
		}
    ]
}

